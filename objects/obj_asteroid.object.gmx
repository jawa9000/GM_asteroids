            <string>//game_restart();

if (sprite_index == spr_player) {
    var randomAngle = random(360); // random angle for debris
    var o; // instance id of dead player objects
    
    o = instance_create(x,y,obj_dead_player);
    o.angle = 0 + randomAngle;
    o = instance_create(x,y,obj_dead_player);
    o.angle = 180 + randomAngle;
    o = instance_create(x,y,obj_dead_player);
    o.angle = 240 + randomAngle;
    
    // move debris out from death location
    with (obj_dead_player) {
        direction = angle;
        image_angle = angle;
        speed = 3;
    }
    
    audio_play_sound(snd_player_exploded,10,0); // play player death sound, no loop

    if (global.life &gt; 0) {
        global.life -= 1;
        instance_create(room_width/2, room_height/2, obj_player);
    } else {
        instance_create(0,0,obj_game_over);
    }
    
    instance_destroy();
}
